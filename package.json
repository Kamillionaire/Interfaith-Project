{
  "name": "interfaith-project",
  "version": "1.0.0",
  "description": "Install bower dependancies `bower i`\r Install Node dependancies `npm i`\r add .env file",
  "main": "./server/bin/www.js",
  "scripts": {
    "preinstall": "npm i webpack typescript nodemon",
    "postinstall": "tsc && webpack",
    "analyze": "ANALYZE=1 npm run build",
    "build:client": "webpack",
    "build:sass": "node-sass ./client/styles/ -o ./client/styles/",
    "build": "tsc && npm run build:client && npm run build:sass && npm run lint",
    "build:prod": "PROD_ENV=1 npm run build",
    "dev": "tsc && concurrently \"webpack -w\" \"node-sass client/styles/ -o client/styles/ -w\" \"nodemon -r dotenv/config\" \"lite-server\"",
    "lint": "tslint -e node_modules './server/**/**/*.ts' './client/**/**/*.ts'",
    "serve": "nodemon -r dotenv/config",
    "debug": "node -r dotenv/config debug server/bin/www.js",
    "inspect": "nodemon -r dotenv/config --inspect server/bin/www.js",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamillionaire/Interfaith-Project.git"
  },
  "engines": {
    "node": "6.9.4",
    "npm": "3.10.10"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kamillionaire/Interfaith-Project/issues"
  },
  "homepage": "https://github.com/Kamillionaire/Interfaith-Project#readme",
  "dependencies": {
